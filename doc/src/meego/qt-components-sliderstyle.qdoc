/*!
    \qmlclass SliderStyle
    \inherits Style
    \ingroup qt-components
    \since 4.7

    \brief
    SliderStyle properties and default values explained.

    Custom platformStyle example

    \code
    Slider {
        platformStyle: SliderStyle {
            fontFamily: "Arial"
            fontPixelSize: 30
            textColor: "red"
            valueBackground: "valueBackground.png"
            labelArrowDown: "labelArrowDown.png"
            labelArrowUp: "labelArrowup.png"
            labelArrowLeft: "labelArrowLeft.png"
            labelArrowRight: "labelArrowRight.png"
            handleBackground: "handleBackground.png"
            handleBackgroundPressed: "handleBackgroundPressed.png"
            grooveItemBackground: "grooveItemBackground.png"
            grooveItemElapsedBackground: "grooveItemElapsedBackground.png"
            mouseMarginTop: 5
            mouseMarginLeft: 5
            mouseMarginRight: 5
            mouseMarginBottom: 5
        }
    }
    \endcode
*/

/*!
    \qmlproperty string SliderStyle::fontFamily

    Property default value is \c UiConstants.DefaultFontFamily.

    Font family.
*/

/*!
    \qmlproperty int SliderStyle::fontPixelSize

    Property default value is \c UI.FONT_DEFAULT_SIZE.

    Font pixel size.
*/

/*!
    \qmlproperty color SliderStyle::textColor

    Property default value is \c !inverted?UI.COLOR_INVERTED_FOREGROUND:UI.COLOR_FOREGROUND.

    Text color.
*/


/*!
    \qmlproperty string SliderStyle::valueBackground

    Property default value is \c "image://theme/meegotouch-slider-handle-value"+__invertedString+"-background".

    Value background image location.
*/

/*!
    \qmlproperty string SliderStyle::labelArrowDown

    Property default value is \c "image://theme/meegotouch-slider-handle-label-arrow-down"+__invertedString.

    Label arrow down image location.
*/


/*!
    \qmlproperty string SliderStyle::labelArrowUp

    Property default value is \c "image://theme/meegotouch-slider-handle-label-arrow-up"+__invertedString.

    Label arrow up image location.
*/


/*!
    \qmlproperty string SliderStyle::labelArrowLeft

    Property default value is \c "image://theme/meegotouch-slider-handle-label-arrow-left"+__invertedString.

    Label arrow left image location.
*/


/*!
    \qmlproperty string SliderStyle::labelArrowRight

    Property default value is \c "image://theme/meegotouch-slider-handle-label-arrow-right"+__invertedString.

    Label arrow right image location.
*/

/*!
    \qmlproperty string SliderStyle::handleBackground

    Property default value is \c "image://theme/meegotouch-slider-handle"+__invertedString+"-background-horizontal".

    Handles background image location.
*/

/*!
    \qmlproperty string SliderStyle::handleBackground

    Property default value is \c "image://theme/meegotouch-slider-handle"+__invertedString+"-background-pressed-horizontal".

    Handles pressed state background image location.
*/

/*!
    \qmlproperty string SliderStyle::grooveItemBackground

    Property default value is \c "image://theme/meegotouch-slider"+__invertedString+"-background-horizontal".

    Grooves item background image location.
*/

/*!
    \qmlproperty string SliderStyle::grooveItemElapsedBackground

    Property default value is \c "image://theme/meegotouch-slider-elapsed"+__invertedString+"-background-horizontal".

    Grooves item elapsed background image location.
*/

/*!
    \qmlproperty real SliderStyle::mouseMarginTop

    Property default value is \c 0.0.

    Mouse margin top.
*/

/*!
    \qmlproperty real SliderStyle::mouseMarginLeft

    Property default value is \c 0.0.

    Mouse margin left.
*/

/*!
    \qmlproperty real SliderStyle::mouseMarginRight

    Property default value is \c 0.0.

    Mouse margin right.
*/

/*!
    \qmlproperty real SliderStyle::mouseMarginBottom

    Property default value is \c 0.0.

    Mouse margin bottom.
*/
